{
    "name": "ernestoch/user-auditable-for-laravel",
    "description": "Laravel package for user auditing in migrations and models",
    "type": "library",
    "keywords": [
        "laravel",
        "audit",
        "user",
        "migration",
        "model",
        "tracking",
        "auditable",
        "macros"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ernesto Chapon",
            "email": "ernestochapon@gmail.com",
            "homepage": "https://github.com/3rn3st0"
        }
    ],
    "require": {
        "php": "^8.3",
        "laravel/framework": "^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10.0",
        "nunomaduro/collision": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "ErnestoCh\\UserAuditable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ErnestoCh\\UserAuditable\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ErnestoCh\\UserAuditable\\Providers\\UserAuditableServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "1.0.1",
    "scripts": {
        "release:patch": "php tagger.php",
        "release:minor": "echo 'Update version in composer.json then run release:patch'",
        "test": "vendor\\bin\\phpunit",
        "test-coverage": "vendor\\bin\\phpunit --coverage-html coverage",
        "test-verbose": "vendor\\bin\\phpunit --verbose"
    }
}
